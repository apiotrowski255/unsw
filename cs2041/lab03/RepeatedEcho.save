if [ $# -ne 2 ]
then
	echo "Usage ./echon.sh <number of lines> <string>"
	exit 1
fi

number = $1

x=0
result=$x $number

if [ result -eq 0 ]
	echo "First argument needs to be a number"
	exit 1
fi




increment=0
while test $increment -lt $number
do
	echo "Hey"
	increment=$(($increment + 1))
done
exit 0
